    ind_max_np(NP1),
    NP1:hdet => no,
    NP1:nform => norm,
    NP1:subn <=> SUBN,
    NP1:has_app => no,
    NP1:redrel => no,
    NP1:cj => ncj.

%% Staking Franse verkeersleiders
