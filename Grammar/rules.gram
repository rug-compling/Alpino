    VP:mf <=> V:mf,
    Mod:wh => nwh,
    Mod:eps <=> V:eps1,  % if Mod can go the left too, then Mod:eps should not
                         % allow this for modifiers that can go both ways
    Mod:sel => to_right,
    Mod:modifies_v => yes,
