    xp_modal_xp_structure(Y,Adv,X).

grammar_rule(xp_modal_xp(vp),X,[Adv,Y]):-
    X => vp,
    X:mf => [],
    X:vform => om,
    Y => vp,
    xp_modal_xp_structure(Y,Adv,X),
    Adv:modal_adv_arg => adv_verb.

grammar_rule(xp_modal_xp(vp_modifier),X,[Adv,Y]) :-
    X => modifier,
    Y => modifier,
    X:mcat => mcat_vp,
    Y:mcat => mcat_vp,
