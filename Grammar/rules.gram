dip_scope_structure(NUCL,ROOT) :-
    structure,
    unify_except_l(NUCL,ROOT,[dt,tags]),
    NUCL:tags <=> [TAG|Tags],
    ROOT:tags <=> Tags,
    alpino_data:dt(ROOT,ROOTDT),
    alpino_data:dt(NUCL,NUCLDT),
    TAG:dt_in <=> NUCLDT,
    TAG:dt_out <=> ROOTDT.

grammar_rule(dip_scope_root,ROOT,[NUCL]) :-
    ROOT => root, NUCL => root,
    dip_scope_structure(NUCL,ROOT).

