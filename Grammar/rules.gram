    NP => np,
    NP:wh => nwh,
    NP:nform => norm,
    NP:subn => (sub_noun;sub_name),
    hd_comp_structure(A,NP,AP).

grammar_rule(a_fixed_a, AP, [ FIXED, A]) :-
