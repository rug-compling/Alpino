grammar_rule(max_xp(sv1),Max,[X]) :-
    max_sv1(X),
    top_max_structure(X,Max),
    Max:dt:stype => ~declarative & ~topic_drop & ~whquestion,
    Max:sv1 => yes,
    Max:puncttype => (punt;uitroep;dubb_punt;punt_komma;vraag;hellip).

