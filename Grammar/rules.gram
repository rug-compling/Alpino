grammar_rule(np_ap,START,[NP,AP]) :-
    START => max,
    START:max_type => t_dp,  % was missing here???
    START:mod_np => yes,
    SUBN => (sub_def_pron;sub_name;sub_noun;sub_indef_verb),
    short_np(NP,SUBN),
    max_nadv_ap(AP),
    dp_dp_structure(NP,AP,START).

