    NP:rightm => yes,
    N:rightm => no,
    n_post_mod_structure(N,CP,NP).

n_mod_structure(N,MOD,NP) :-
    NP => n, N => n, 
    N:bmeas <=> NP:bmeas,
    hd_mod_structure(N,MOD,NP).

n_post_mod_structure(N,MOD,NP) :-
    n_mod_structure(N,MOD,NP),
    haspreno(NP). %%%    NP:haspre => no.  % for generation we can't have this

grammar_rule(n_n_mod_vp, NP, [N, VP ]) :-
