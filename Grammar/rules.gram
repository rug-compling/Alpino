%% PPs
grammar_rule(mod2, Mod, [ Pp ] ) :-
    Mod => modifier, Pp => pp,

    No => no,
    Yes => yes,
    Pp:pp_er <=> ER,
    Mod:sel <=> Sel,
    NotRight => ~to_right,

    when(nonvar(ER),
    	 (  ER = No
    	 ;  ER = Yes, Sel = NotRight
    	 )),
    
    Mod:mcat => mcat_pp,
    Mod:eps => no,
    Pp:cj => ncj, % diy
    Mod:cj => ncj,
    Pp:e_deps => [],    % otherwise spurious ambiguities
    Pp:nnform => ~refl, % id.
    Pp:ld_pc => (mod_pp;pred_pp),
    Pp:pp_role => pp_obj1,
				% Pp:vc => yes,
    hd_structure(Pp,Mod ).

