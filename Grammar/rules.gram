nominalization_cat(N) :-
    N => n,
    N:exs => [],
    N:agr => het&sg,
    N:lex => yes,
    N:nform => norm.

grammar_rule(nominalization,N,[V]) :-
    nominalization_cat(N),
    V => vpx_noun,
    finish_mexs(V),
    hd_structure(V,N0),
    unify_except_l(N0,N1,[hstem,lex]),
    N:hstem ==> nominalization(Vstem),
    V:hstem ==> Vstem,
    overwrite(N1,N,mexs,[]). % island for relatives etc...

