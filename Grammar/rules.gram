unify_paths([],_,_).
unify_paths([H|T],A,B) :-
    A:H <?=?> B:H,
    unify_paths(T,A,B).

unify_vb_v(Sign0,Sign) :-
    Sign => vb,
    Sign0 => v,
    alpino_types:type(v,_,Atts),
    unify_each(Atts,Sign,Sign0).

unify_vc_vb(Sign0,Sign) :-
    Sign => vc,
    Sign0 => vb,
    alpino_types:type(vb,_,Atts),
    unify_each(Atts,Sign,Sign0).

unify_each([],_,_).
unify_each([Att|Atts],Sign0,Sign) :-
    Sign0:Att <?=?> Sign:Att,
    unify_each(Atts,Sign0,Sign).

