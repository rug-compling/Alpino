    CLOSE => punct, CLOSE:puncttype => ligg_streep,
    optend_mod_punct(END),
    VP => vproj, V => vproj, 
    optcomma(CommaR),
    V:realized_comma <=> CommaR:realized,
    vp_predm_v_structure(VP,Mod,V).

grammar_rule(vp_predm_v, VP, [ Mod, CommaR, V ] ) :-
