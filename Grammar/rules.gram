    CrdType => two_crd,
    M:cj <=> CrdType.

zowel_als_start_structure(Name,M,CrdType,ConjL,XP,Clist) :-
    Clist => clist,
    Clist:etc <=> ETC,
    start_coord_daughter(Name,XP,M,CrdType,[],ETC),
    ConjL => lconj,
    unify_mods(ConjL),
    ConjL:dt <=> ConjLDt,    
    Clist:cat <=> M,
    Clist:conj <=> ConjRDt,
    Clist:left_conj => yes,
    Clist:cats <=> Dts,
    Clist:cform <=> ConjL:needs_right_cform,
    start_coord_structure(Name,[ConjLDt|ConjRDt],M,XP,[XPdt|Dts],Clist),
    XP:dt <=> XPdt.

