    (   hdrug_util:hdrug_flag(parse_or_generate,parse),
	Crd => ~no_crd
    ;   hdrug_util:hdrug_flag(parse_or_generate,generate),
	Crd => ~ncj
    ),
    M => adv,
    M:wk => no,
    M:tmploc => ~wk_er & ~str_er.

conjunction_cat(root,M,Crd,_) :-
    (   hdrug_util:hdrug_flag(parse_or_generate,parse),
	Crd => one_crd
    ;   hdrug_util:hdrug_flag(parse_or_generate,generate),
	Crd => ~ncj
    ),
    max_root(M0),
    unify_except(M0,M,mods),
    M:tags => [],
    M:needs_dip => no.

conjunction_cat(a,M,Crd,ETC) :-
    ETC => no,
    (   hdrug_util:hdrug_flag(parse_or_generate,parse),
