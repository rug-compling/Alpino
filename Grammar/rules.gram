    DIPP => dip,
    DIP => sv1,
    DIP:subj:allows_drop => no,
    DIP:can_be_max => yes,
    DIP:slash <=> [SbarArg],
    DIP:iexs => [],
    DIP:imexs => [],
    DIP:tags => [],
    DIP:haswh => no,
    unify_mods(DIP),
    max_sbar(SbarArg),
    SbarArg:ctype => c_dip,
    SbarArg:dt => [], % !
    projected_hd_filler_structure(DIP,SbarArg,DIPP).

grammar_rule(dip_root,DIPP,[DIP]) :-
    DIPP => dip,
