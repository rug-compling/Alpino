	XP:subj <=> Subj,
	XP:topic_hstem <=> Topic
    ;   XP /=> root,
	Subj => np,
	Subj:nform => none,
	Topic ==> none
    ),
    M:topic_hstem <=> Topic,
    M:subj <?=?> Subj.


grammar_rule(start_coord(Name,zowel_als),M,[ConjL, XP, Clist]) :-
