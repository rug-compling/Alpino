    Q:iexs => [],
    Q:imexs => [],
    SLASH => modifier,
    SLASH:hstem ==> dip_adv,  % so not "er", for er_def 
    SLASH:exs =?> [],
    SLASH:mexs =?> [],
    ZO:dt <=> SLASH:dt,
    projected_hd_filler_structure(Q,SLASH,ROOT,smain).

