grammar_rule(adv_tmp_np,Adv,[NP]) :-
    Adv0 => adv,
    unify_except(Adv0,Adv,wh_reltmploc),
    Adv:wh_reltmploc => tmp_n,
    Adv:subadv => subadv_noun,
    Adv:tmploc => tmp_n,
    NP => np,
    NP:redrel => no,
    NP:pred => npred,
    NP:nform => (year;temp),
    NP:e_deps => [],
    NP:cj => ncj,
    NP:bmeas => no,
