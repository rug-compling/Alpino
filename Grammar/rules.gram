qr(PUNCT) :-
    PUNCT => punct,
    PUNCT:puncttype => aanhaal_rechts.

optql(OPTQL) :-
    OPTQL => optpunct,
    OPTQL:cat <=> Punct,
    ql(Punct).

optqr(OPTQL) :-
    OPTQL => optpunct,
    OPTQL:cat <=> Punct,
    qr(Punct).

