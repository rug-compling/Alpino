    VP => vproj, V => vproj, Mod => modifier,
    (   hdrug_util:hdrug_flag(parse_or_generate,parse)
    ;   hdrug_util:hdrug_flag(parse_or_generate,generate),
	Mod:mcat => ~mcat_sbar & ~mcat_redrel & ~mcat_imp
